{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "New NFS FileSystem: NFSv3-Test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/New-PureNFSFileSystem.ps1",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "New NFSv3 Datastore (Hourly Snapshots)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/New-NFSDatastore.ps1",
            "args": [
                "-DatastoreName", "NFSv3-Hourly",
                "-DatastoreSize", "10TB",
                "-NFSVersion", "nfsv3",
                "-vCenterCluster", "cluster-prod",
                "-FlashArrayEndpoint", "sn1-x90r2-f07-27.fsa.lab",
                "-vCenterServer", "vc02.fsa.lab",
                "-QuotaEnabled", "$true",
                "-SnapshotEnabled", "$true",
                "-SnapshotRulesEvery", "3600000",
                "-SnapshotRulesKeepFor", "86400000",
                "-SnapshotClientName", "hourly"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "New NFSv4 Datastore (Hourly Snapshots + Nconnect)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/New-NFSDatastore.ps1",
            "args": [
                "-DatastoreName", "NFSv4-Hourly",
                "-DatastoreSize", "20TB",
                "-NFSVersion", "nfsv4",
                "-vCenterCluster", "cluster-prod",
                "-NconnectSessions", "8",
                "-FlashArrayEndpoint", "sn1-x90r2-f07-27.fsa.lab",
                "-vCenterServer", "vc02.fsa.lab",
                "-QuotaEnabled", "$true",
                "-SnapshotEnabled", "$true",
                "-SnapshotRulesEvery", "3600000",
                "-SnapshotRulesKeepFor", "86400000",
                "-SnapshotClientName", "hourly"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Remove-NFSDatastoreFromFlashArray: NFSv3-Test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Remove-NFSDatastoreFromFlashArray.ps1",
            "args": [
                "-DatastoreName", "NFSv3-Test",
                "-FlashArrayEndpoint", "sn1-x90r2-f07-27.fsa.lab"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Remove-NFSDatastoreFromFlashArray: NFSv4-Test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Remove-NFSDatastoreFromFlashArray.ps1",
            "args": [
                "-DatastoreName", "NFSv4-Test",
                "-FlashArrayEndpoint", "sn1-x90r2-f07-27.fsa.lab"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Mount Existing NFS: NFSv3-Test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/New-NFSDatastore.ps1",
            "args": [
                "-DatastoreName", "NFSv3-Test",
                "-NFSExportPath", "/NFSv3-Test",
                "-NFSHost", "filevip-f07-27.fsa.lab",
                "-vCenterCluster", "cluster-prod",
                "-vCenterServer", "vc02.fsa.lab",
                "-NFSVersion", "nfsv3"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Mount Existing NFS: NFSv4-Test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/New-NFSDatastore.ps1",
            "args": [
                "-DatastoreName", "NFSv4-Test",
                "-NFSExportPath", "/NFSv4-Test",
                "-NFSHost", "filevip-f07-27.fsa.lab",
                "-vCenterCluster", "cluster-prod",
                "-vCenterServer", "vc02.fsa.lab",
                "-NFSVersion", "nfsv4",
                "-NconnectSessions", "8"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "PowerShell: Interactive Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": false
        }
    ]
}